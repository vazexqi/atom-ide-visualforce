{
  "name": "atom-ide-visualforce",
  "main": "./lib/extension",
  "version": "0.0.1",
  "description": "Provides support for the Visualforce Language Server in Atom.",
  "keywords": [],
  "repository": "https://github.com/vazexqi/atom-ide-visualforce",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0"
  },
  "scripts": {
    "postinstall": "npm install patch/atom-languageclient-0.6.5.tgz"
  },
  "dependencies": {
    "@salesforce/salesforcedx-visualforce-language-server": "41.4.0",
    "atom-languageclient": "0.6.5",
    "typescript": "2.4.0"
  },
  "devDependencies": {
    "prettier": "^1.7.0",
    "shelljs": "^0.7.8"
  },
  "enhancedScopes": [
    "source.visualforce"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
